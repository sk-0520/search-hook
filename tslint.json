{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "array-type": false,
        "no-trailing-whitespace": false,
        "typedef-whitespace": [
            true,
            {
                "callSignature": "noSpace",
                "catchClause": "noSpace",
                "indexSignature": "noSpace"
            }
        ],
        "whitespace": [
            true,
            "check-decl",
            "check-operator",
            "check-separator",
            "check-type"
        ],
        "quotemark": [
            false
        ],
        "arrow-parens": false,
        "trailing-comma": [
            false
        ],
        "comment-format": [
            false
        ],
        "no-unused-expression": [
            true,
            "allow-fast-null-checks",
            "allow-new",
            "allow-tagged-template"
        ],
        "max-classes-per-file": [
            false
        ],
        "object-literal-sort-keys": false,
        "no-console": [
            false
        ],
        "max-line-length": [
            true,
            150
        ],
        "prefer-for-of": false,
        "no-for-in-array": false,
        "no-empty-interface": false,
        "no-consecutive-blank-lines": false,
        "ordered-imports": false
    },
    "rulesDirectory": []
}