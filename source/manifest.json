{

  "description": "Demonstrating webRequests",
  "manifest_version": 2,
  "name": "op-search",
  "version": "1.0",

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "<all_urls>"
  ],

  "background": {
    "scripts": ["scripts/background.js"]
  },

  "options_ui": {
    "page": "options/options.html"
  }

}
