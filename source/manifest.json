{

  "description": "Demonstrating webRequests",
  "manifest_version": 2,
  "name": "op-search",
  "version": "1.0",

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "<all_urls>"
  ],

  "background": {
    "scripts": [
        "scripts/common.js",
        "scripts/redirect-google.js",
        "scripts/redirect-background.js"
    ]
  },

  "options_ui": {
    "page": "view/options.html"
  }

}
