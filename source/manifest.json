{

  "description": "Demonstrating webRequests",
  "manifest_version": 2,
  "name": "op-search",
  "version": "1.0",

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "<all_urls>"
  ],

  "background": {
    "scripts": [
        "script/common.js",
        "script/redirect-google.js",
        "script/redirect-bing.js",
        "script/redirect-background.js"
    ]
  },

  "options_ui": {
    "page": "view/options.html"
  }

}
